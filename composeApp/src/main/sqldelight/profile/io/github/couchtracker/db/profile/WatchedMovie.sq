import io.github.couchtracker.db.profile.externalids.ExternalMovieId;

CREATE TABLE WatchedMovie (
  id INTEGER PRIMARY KEY NOT NULL,
  movieId TEXT AS ExternalMovieId NOT NULL,

  FOREIGN KEY(id) REFERENCES WatchedItem(id) ON UPDATE CASCADE ON DELETE CASCADE
);

selectAll:
SELECT * FROM WatchedMovie;

insert:
INSERT INTO WatchedMovie(id, movieId)
VALUES (:id, :movieId);
